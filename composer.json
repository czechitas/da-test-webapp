{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/arxeiss/BaconQrCode"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "bacon/bacon-qr-code": "dev-add-gd-backend#650e10121f4d8707ecdc5984b042cae7731e388a",
        "erusev/parsedown-extra": "0.8.1",
        "guzzlehttp/guzzle": "7.5.0",
        "granam/czech-vocative": "2.2.0",
        "ifsnop/mysqldump-php": "2.10",
        "laravel/framework": "10.1.3",
        "laravel/sanctum": "3.2.1",
        "laravel/ui": "4.2.1",
        "league/flysystem-aws-s3-v3": "3.12.2",
        "league/flysystem-ftp": "3.10.3",
        "mpdf/mpdf": "8.1.4",
        "phpoffice/phpspreadsheet": "1.27.1",
        "prologue/alerts": "1.1.0",
        "resampler/resampler": "1.0",
        "tamtamchik/namecase": "3.0.0"
    },
    "require-dev": {
        "arxeiss/coding-standards": "0.11.0",
        "barryvdh/laravel-debugbar": "3.8.1",
        "fakerphp/faker": "1.21.0",
        "laravel/tinker": "2.8.1",
        "mockery/mockery": "1.5.1",
        "nunomaduro/collision": "7.0.5",
        "nunomaduro/larastan": "2.4.1",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "phpunit/phpunit": "10.0.11",
        "spatie/laravel-ignition": "2.0.0",
        "heroku/heroku-buildpack-php": "*"
    },
    "autoload": {
        "files": ["app/helpers.php"],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "CzechitasApp\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ],
        "post-install-cmd": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan migrate --force"
        ],
        "warmup": [
            "php artisan warmup"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
